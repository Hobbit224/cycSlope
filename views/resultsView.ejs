

<div id="carousel-example-generic" class="my-carousel carousel slide" data-ride="carousel">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
    <li data-target="#carousel-example-generic" data-slide-to="1"></li>
    <li data-target="#carousel-example-generic" data-slide-to="2"></li>
  </ol>

  <!-- Wrapper for slides -->
  <div class="carousel-inner" role="listbox">
    <div class="item active">
        <!-- mapApiUrl div -->
        <div class="text-center map-div" id="map1_div"><img class="map" src="<%= mapApiUrl%>" /></div>
        <!-- elevationData chart -->
        <div class="text-center" id="chart1_div"></div>
        <!-- Distance / Change in Elevation / Time div -->
        <div class="carousel-caption caption">
        <ul>
            <li class="carousel-list">Distance: <span class="distance-result-1">X</span></li>
            <li class="carousel-list">Change in Elevation: <span class="elevation-result-1">X</span></li>
            <li class="carousel-list">Time: <span class="time-result-1">X</span></li>
        </ul>
      </div>
    </div>
    <div class="item">
        <!-- mapApiUrl div -->
        <div class="text-center map-div" id="map2_div"><img class="map" src="http://via.placeholder.com/320x320" /></div>
        <!-- elevationData chart -->
        <div class="text-center" id="chart2_div"></div>
        <!-- Distance / Change in Elevation / Time div -->
        <div class="carousel-caption caption">
            <ul>
                <li class="carousel-list">Distance: <span class="distance-result-2">X</span></li>
                <li class="carousel-list">Change in Elevation: <span class="elevation-result-2">X</span></li>
                <li class="carousel-list">Time: <span class="time-result-2">X</span></li>
            </ul>
        </div>
    </div>
    <div class="item">
        <!-- mapApiUrl div -->
        <div class="text-center map-div" id="map3_div"><img class="map" src="http://via.placeholder.com/320x320" /></div>
        <!-- elevationData chart -->
        <div class="text-center" id="chart3_div"></div>
        <!-- Distance / Change in Elevation / Time div -->
        <div class="carousel-caption caption">
            <ul>
                <li class="carousel-list">Distance: <span class="distance-result-3">X</span></li>
                <li class="carousel-list">Change in Elevation: <span class="elevation-result-3">X</span></li>
                <li class="carousel-list">Time: <span class="time-result-3">X</span></li>
            </ul>
        </div>
    </div>
  </div>

  <!-- Controls -->
  <a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
    <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
    <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>