<!DOCTYPE html>
<html>
<head>
	<title>cycSlope User Profile</title>

	<% include head %>
</head>
<body>

	<% include navbar %>


	<div class="container">
		<div class="main-profile-box">
			<div class="row">
				<div class="user-header col-xs-12 text-center">
					<h1 class="profile-header"><%=firstName%>'s Profile</h1>
				</div>
			</div>
			<div class="row">
				<div class="user-info-area">
					<div class="prof-info-input col-xs-3 col-xs-offset-1">
						<div class="info-input info-input-1">First Name:</div>
						<div class="info-input info-input-2">Username:</div>
						<div class="info-input info-input-3">Email:</div>
						<div class="info-input info-input-4">Gender:</div>
					</div>
					<div class="prof-info-input col-xs-3">
						<div class="info-output info-output-1">
							<%=firstName%>
						</div>
						<div class="info-output info-output-2">
							<%=username%>
						</div>
						<div class="info-output info-output-3">
							<%=email%>
						</div>
						<div class="info-output info-output-4">
							<%=gender%>
						</div>
					</div>
				</div>
			</div>
		</div>


</body>
</html>