<!DOCTYPE html>
<html>
<head>
	<title>cycSlope User Profile</title>

	<% include head %>
</head>
<body>

	<% include navbar %>

	<div class="container">
		<div class="main-profile-box">
			<div class="row">
				<div class="user-header col-xs-12 text-center">
					<h1>User Profile</h1>
				</div>
			</div>
			<div class="row">
				<div class="prof-pic col-xs-4">
					<!-- This is the user picture -->
				</div>
				<div class="user-info-area">
					<div class="prof-info-input col-xs-3 col-xs-offset-1">
						<div class="info-input info-input-1">First Name</div>
						<div class="info-input info-input-2">Username</div>
						<div class="info-input info-input-3">Email</div>
						<div class="info-input info-input-4">Gender</div>
						<div class="info-input info-input-5">Saved Locations</div>
						<div class="info-input info-input-6">Saved Routes</div>
					</div>
					<div class="prof-info-input col-xs-3 col-xs-offset-1">
						<div class="info-output info-output-1">#</div>
						<div class="info-output info-output-2">#</div>
						<div class="info-output info-output-3">#</div>
						<div class="info-output info-output-4">#</div>
						<div class="info-output info-output-5">#</div>
						<div class="info-output info-output-6">#</div>
					</div>
				</div>
			</div>
		</div>


</body>
</html>